<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="363.0" prefWidth="693.0"
            xmlns="http://javafx.com/javafx/8.0.40" fx:controller="org.ftccommunity.gui.view.OverviewController">
    <SplitPane dividerPositions="0.5" layoutX="153.0" layoutY="70.0" prefHeight="300.0" prefWidth="600.0"
               AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
               AnchorPane.topAnchor="0.0">
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
            <TableView fx:id="brickTable" layoutX="-12.0" layoutY="49.0" prefHeight="298.0" prefWidth="282.0"
                       AnchorPane.bottomAnchor="65.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                       AnchorPane.topAnchor="0.0">
                <columns>
                    <TableColumn fx:id="brickNameColumn" prefWidth="75.0" text="Controller"/>
                    <TableColumn fx:id="brickAliasColumn" prefWidth="75.0" text="Name"/>
                </columns>
                <columnResizePolicy>
                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                </columnResizePolicy>
            </TableView>
            <GridPane prefHeight="30.0" prefWidth="342.0" AnchorPane.bottomAnchor="20.0"
                      AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="1.0">
                <columnConstraints>
                    <ColumnConstraints halignment="CENTER" hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0"/>
                    <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0"/>
                    <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                </rowConstraints>
                <Button mnemonicParsing="false" onAction="#handleNewBrick" prefWidth="80.0" text="New..."
                        GridPane.halignment="CENTER"/>
                <Button mnemonicParsing="false" onAction="#handleEditBrick" prefWidth="80.0" text="Edit..."
                        GridPane.columnIndex="1" GridPane.halignment="CENTER"/>
                <Button mnemonicParsing="false" onAction="#handleDeleteBrick" prefWidth="80.0" text="Delete"
                        GridPane.columnIndex="2" GridPane.halignment="CENTER"/>
            </GridPane>
        </AnchorPane>
        <SplitPane dividerPositions="0.6121883656509696" orientation="VERTICAL" prefHeight="361.0" prefWidth="0.0">
            <AnchorPane>
                <Label layoutX="10.0" layoutY="8.0" styleClass="label-header" text="Controller Details"
                       AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="8.0">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Label>
                <GridPane layoutX="5.0" layoutY="34.0" prefHeight="52.0" prefWidth="332.0"
                          AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0"
                          AnchorPane.topAnchor="34.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <Label text="Name"/>
                        <Label fx:id="brickNameLabel" styleClass="label-bright" text="Label"
                               GridPane.columnIndex="1"/>
                        <Label text="UDP Port" GridPane.rowIndex="1"/>
                        <Label fx:id="brickPortLabel" styleClass="label-bright" text="Label"
                               GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                        <Label text="Serial Number" GridPane.rowIndex="2"/>
                        <Label fx:id="brickSerialLabel" styleClass="label-bright" text="Label"
                               GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                    </children>
                </GridPane>
                <Pane fx:id="detailsPane" layoutX="4.0" layoutY="94.0" prefHeight="168.0"
                      AnchorPane.leftAnchor="4.0" AnchorPane.rightAnchor="6.0"/>
            </AnchorPane>
            <AnchorPane fx:id="buttonStartSimulator" minHeight="0.0" minWidth="0.0" prefHeight="100.0"
                        prefWidth="160.0">
                <GridPane layoutX="14.0" layoutY="26.0" prefHeight="102.0" prefWidth="342.0"
                          AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <Button mnemonicParsing="false" onAction="#handleStartSimulatorButton"
                                text="Start Simulator">
                            <GridPane.margin>
                                <Insets left="10.0"/>
                            </GridPane.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#handleStartDebugButton"
                                text="Start Debugger" GridPane.rowIndex="1">
                            <GridPane.margin>
                                <Insets left="10.0"/>
                            </GridPane.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#handleStartVisualizerButton"
                                text="Start Visualizer" GridPane.rowIndex="2">
                            <GridPane.margin>
                                <Insets left="10.0"/>
                            </GridPane.margin>
                        </Button>
                    </children>
                </GridPane>
            </AnchorPane>
        </SplitPane>
    </SplitPane>
</AnchorPane>
